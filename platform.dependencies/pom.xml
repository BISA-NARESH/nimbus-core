			</dependency>
			<dependency>
				<groupId>com.anthem.nimbus.platform</groupId>
				<artifactId>com.anthem.nimbus.platform.utils.rest.client</artifactId>
				<version>${nimbus.utils.restclients.version}</version>
			</dependency>
			<dependency>
				<groupId>com.anthem.nimbus.platform</groupId>
				<artifactId>com.anthem.nimbus.platform.utils.reference.data</artifactId>
				<version>${nimbus.utils.refdata.version}</version>
			</dependency>
			<dependency>
				<groupId>com.anthem.nimbus.platform</groupId>
				<artifactId>com.anthem.nimbus.platform.core.process</artifactId>
				<version>${nimbus.process.version}</version>
			</dependency>
			<dependency>
				<groupId>com.anthem.nimbus.platform</groupId>
				<artifactId>com.anthem.nimbus.platform.core.process</artifactId>
				<version>${nimbus.process.version}</version>
				<type>test-jar</type>
			</dependency>
			<dependency>
				<groupId>com.anthem.nimbus.platform</groupId>
				<artifactId>com.anthem.nimbus.platform.starter-spec-model</artifactId>
				<version>${nimbus.model.starter.version}</version>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>com.anthem.nimbus.platform</groupId>
				<artifactId>com.anthem.nimbus.platform.starter-core-process</artifactId>
				<version>${nimbus.process.starter.version}</version>
				<type>pom</type>
			</dependency>
			<!-- Third Party must have dependencies -->
			<dependency>
				<groupId>org.activiti</groupId>
				<artifactId>activiti-spring-boot-starter-basic</artifactId>
				<version>${activiti-spring-boot.version}</version>
			</dependency>
			<dependency>
				<groupId>io.springfox</groupId>
				<artifactId>springfox-swagger2</artifactId>
				<version>${swagger.version}</version>
			</dependency>
			<dependency>
				<groupId>io.springfox</groupId>
				<artifactId>springfox-swagger-ui</artifactId>
				<version>${swagger.version}</version>
			</dependency>
			<dependency>
				<groupId>commons-io</groupId>
				<artifactId>commons-io</artifactId>
				<version>${commons-io.version}</version>
			</dependency>
			<dependency>
				<groupId>org.apache.commons</groupId>
				<artifactId>commons-lang3</artifactId>
				<version>${commons-lang.version}</version>
			</dependency>
			<dependency>
				<groupId>org.drools</groupId>
				<artifactId>drools-core</artifactId>
				<version>${drools.version}</version>
			</dependency>
			<dependency>
				<groupId>org.drools</groupId>
				<artifactId>drools-compiler</artifactId>
				<version>${drools.version}</version>
